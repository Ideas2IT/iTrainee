
@model iTrainee.Models.UserAudit;
@{
    ViewData["Title"] = "Trainee";
}
<div class="row mt-3">
    <div class="col">
        
    <p>Date : @Model.Date.ToShortDateString()</p>
    </div>
    <div class="col">
   
    <p>Sign-In : @Model.SignIn.ToShortTimeString()</p>
    </div>
    <p>Sign-Out : @Model.SignOut.ToShortTimeString()</p>
 
</div>
       

<h3 class="font-weight-bold mt-3 mb-5">Hello @TempData.Peek("UserFirstName"),</h3>
<div class="col">
    <div class="row">
    <p>C# Basics</p>
    <div class="progress" style="width: 75%">
  <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
</div>
</div>
          <div class="row">
    <p>C# Advanced</p>
    <div class="progress" style="width: 75%">
  <div class="progress-bar" role="progressbar" style="width: 25%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">25%</div>
</div>
</div>
<div class="row">
    <p>Console Application</p>
    <div class="progress" style="width: 75%">
  <div class="progress-bar" role="progressbar" style="width: 50%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">50%</div>
</div>
</div>
    <p>Windows Application</p>
    <p>MVC Application</p>
    <p>Web Api Application</p>
</div>

      